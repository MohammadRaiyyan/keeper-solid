{
  "$schema": "https://biomejs.dev/schemas/1.9.4/schema.json",
  "vcs": {
    "enabled": false,
    "clientKind": "git",
    "useIgnoreFile": false
  },
  "files": {
    "ignoreUnknown": false,
    "ignore": [
      "/tsconfig.app.json",
      "/tsconfig.node.json",
      "/tsconfig.json",
      "/src/routeTree.gen.ts"
    ]
  },
  "formatter": {
    "enabled": true,
    "indentStyle": "space"
  },
  "organizeImports": {
    "enabled": true
  },
  "linter": {
    "enabled": true,
    "rules": {
      "recommended": true
    },
    "ignore": [
      "tsconfig.node.json",
      "tsconfig.app.json",
      "src/routeTree.gen.ts"
    ]
  },
  "javascript": {
    "formatter": {
      "quoteStyle": "double"
    }
  }
}
